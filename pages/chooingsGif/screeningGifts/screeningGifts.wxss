/* pages/chooingsGif/screeningGifts/screeningGifts.wxss */
page{
  width: 100%;
  height: 100%;
}
.loadBox {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.loadImg {
  width: 420rpx;
  height: 120rpx;
}
.proLoading{
  height: 12rpx;
  border-radius: 6rpx;
  margin: 10rpx 0 50rpx 0;
  background: #FAE6EE;
  position: relative;
}
.animateLoad{
  position: absolute;
  top: 0;
  left: 0;
  width: 60rpx;
  height: 12rpx;
  border-radius: 6rpx;
  animation: loading ease-in 2.4s infinite;
}
@keyframes loading{
  from{
    width: 0%
  }
  to{
    width: 100%;
  }
}