.itemBox {
  height: 340rpx;
  margin: -58rpx 30rpx 0 30rpx;
  border: 2rpx solid #91b6e2;
  border-radius: 12rpx;
  background: #fff;
  box-sizing: border-box;
}
.itemTitle{
  height: 114rpx;
  line-height: 114rpx;
  margin: 0 40rpx;
  border-bottom: 2rpx solid #DEDEDE;
}
.titles{
  font-size: 36rpx;
  font-family: 'HiraginoSansGB';
  font-weight: 600;
  margin-right: 30rpx;
}
.des{
  font-size: 24rpx;
}
.itemIcon{
  width: 78rpx;
  height: 78rpx;
  margin: 40rpx 0 38rpx 0;
}
.itemList{
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.items{
  text-align: center;
}
